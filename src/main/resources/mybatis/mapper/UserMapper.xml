<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yiguan.douban.mapper.UserMapper">

    <select id="selectUsers" resultType="com.yiguan.douban.entity.User">
        SELECT *
        FROM `douban_analysys`.`user`
        where inferred_sex is null
        LIMIT 5000,1000
    </select>
</mapper>